<Window x:Class="__InformationTechnology__Circle.Authorization.Admin.AdminMainPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:__InformationTechnology__Circle.Authorization.Admin"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="AdminMainPage">

    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/Authorization/Admin/PicturesAdmin/Image1.png"/>
        </Grid.Background>
        <Button x:Name="Polzovatel" Content="Пользователи" HorizontalAlignment="Left" Margin="52,103,0,0" VerticalAlignment="Top" Height="18" Width="84" BorderBrush="{x:Null}" Background="{x:Null}" Foreground="#FFDEE2FF" Click="ButtonPolzovatel_Click"/>
        <Button x:Name="Zvonki" Content="Звонки" HorizontalAlignment="Left" Margin="52,130,0,0" VerticalAlignment="Top" Height="18" Width="48" BorderBrush="{x:Null}" Background="{x:Null}" Foreground="#FFDEE2FF" Click="ButtonZvonki_Click"/>
        <Button x:Name="Kurs" Content="Курсы" HorizontalAlignment="Left" Margin="52,157,0,0" VerticalAlignment="Top" Height="18" Width="48" BorderBrush="{x:Null}" Background="{x:Null}" Foreground="#FFDEE2FF" Click="ButtonKurs_Click"/>




    </Grid>
</Window>
